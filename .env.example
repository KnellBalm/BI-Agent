# LLM API Keys
GEMINI_API_KEY=your_gemini_api_key_here
# Model Selection (gemini-2.0-flash, gemini-1.5-flash, gemini-1.5-pro)
GEMINI_MODEL=gemini-2.0-flash
# Local LLM Fallback (Ollama)
OLLAMA_MODEL=llama3
# Multi-key and Quota Control (JSON string)
# Example: '[{"key": "KEY1", "type": "free", "project_id": "PID1"}, {"key": "KEY2", "type": "paid", "project_id": "PID2"}]'
GEMINI_API_CONFIGS='[]'
GCP_PROJECT_ID=your_gcp_project_id_here

# BI Solution
BI_JSON_PATH=tmp/your_report.json

# Server Configuration
SERVER_PORT=3000
API_PORT=8000

# MCP Servers (Configuration example)
MCP_EXCEL_PORT=3003

# Development
DEBUG=true
LOG_LEVEL=info
